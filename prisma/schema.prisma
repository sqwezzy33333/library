generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @db.Uuid
  login     String
  password  String
  varsion   Int
  createdAt Int
  updatedAt Int
}

model Track {
  id       String  @id @db.Uuid
  name     String
  artistId String?
  albumId  String?
  duration Int
}

model Artist {
  id     String  @id @db.Uuid
  name   String
  grammy Boolean
}

model Album {
  id       String  @id @db.Uuid
  name     String
  year     Int
  artistId String?
}
